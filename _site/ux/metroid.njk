  <body class="bg-neutral-900 font-mono uppercase !block" style="display: none;" x-data="metroidPasswordApp()" x-init="updatePassword()">

    <div class="pb-4 px-4 min-h-screen flex flex-col">
      <header class="pt-12 pb-8 mb-12 border-b border-dashed border-neutral-500/50 shrink-0">
        <h1 class="text-success text-2xl text-center font-black md:text-[3.5vw] md:leading-tight md:tracking-tighter">Metroid Password Generator</h1>
      </header>

      <main class="mx-auto max-w-5xl space-y-8 grow">

        <!-- password -->
