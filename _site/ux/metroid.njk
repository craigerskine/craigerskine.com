          <h2>Start In</h2>
          <div class="flex flex-wrap gap-2">
            <label><input type="radio" name="start" class="btn btn-outline btn-primary btn-sm uppercase" value="brinstar" x-model="startLocation" @change="updatePassword()" aria-label="Brinstar" /></label>
            <label><input type="radio" name="start" class="btn btn-outline btn-primary btn-sm uppercase" value="norfair" x-model="startLocation" @change="updatePassword()" aria-label="Norfair" /></label>
            <label><input type="radio" name="start" class="btn btn-outline btn-primary btn-sm uppercase" value="kraidsLair" x-model="startLocation" @change="updatePassword()" aria-label="Kraid's Lair" /></label>
            <label><input type="radio" name="start" class="btn btn-outline btn-primary btn-sm uppercase" value="ridleysLair" x-model="startLocation" @change="updatePassword()" aria-label="Ridley's Lair" /></label>
            <label><input type="radio" name="start" class="btn btn-outline btn-primary btn-sm uppercase" value="tourian" x-model="startLocation" @change="updatePassword()" aria-label="Tourian" /></label>
          </div>
        </section>

        <!-- power-ups -->
         <section>
          <h2>Power-Ups</h2>
